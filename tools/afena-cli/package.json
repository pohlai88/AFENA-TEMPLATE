{
  "name": "@afenda/cli",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": { "afenda": "bin/afenda" },
  "engines": { "node": ">=18" },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --cache --concurrency=auto",
    "lint:ci": "eslint src --ext .js,.jsx,.ts,.tsx --rule 'import/no-cycle: error'",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --cache --fix",
    "type-check": "tsc --noEmit",
    "meta:check": "node dist/index.js meta check"
  },
  "dependencies": {
    "commander": "catalog:",
    "cosmiconfig": "catalog:",
    "execa": "catalog:",
    "fast-glob": "catalog:",
    "listr2": "catalog:",
    "picocolors": "catalog:",
    "semver": "catalog:",
    "strip-ansi": "catalog:",
    "zod": "catalog:",
    "afenda-canon": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/semver": "catalog:",
    "afenda-eslint-config": "workspace:^",
    "afenda-typescript-config": "workspace:*",
    "eslint": "catalog:",
    "ts-morph": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}
