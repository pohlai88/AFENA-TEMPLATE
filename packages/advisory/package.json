{
  "name": "afena-advisory",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache --concurrency=auto",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx --rule 'import/no-cycle: error'",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "type-check": "tsc --noEmit",
    "test": "vitest run"
  },
  "dependencies": {
    "afena-canon": "workspace:*",
    "afena-database": "workspace:*",
    "drizzle-orm": "^0.44.0"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "afena-eslint-config": "workspace:^",
    "afena-typescript-config": "workspace:*",
    "tslib": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:",
    "vitest": "catalog:",
    "afena-vitest-config": "workspace:*"
  }
}