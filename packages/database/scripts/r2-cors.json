{
  "rules": [
    {
      "allowed": {
        "origins": [
          "http://localhost:3000",
          "https://nexuscanon.com",
          "https://*.nexuscanon.com"
        ],
        "methods": [
          "PUT"
        ],
        "headers": [
          "Content-Type"
        ]
      },
      "exposeHeaders": [
        "ETag"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}