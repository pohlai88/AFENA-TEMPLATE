{
  "rules": [
    {
      "allowed": {
        "origins": [
          "http://localhost:3000",
          "https://nexuscanon.com",
          "https://www.nexuscanon.com"
        ],
        "methods": [
          "PUT",
          "GET",
          "HEAD"
        ],
        "headers": [
          "Content-Type",
          "Content-MD5",
          "x-amz-*"
        ]
      },
      "exposeHeaders": [
        "ETag"
      ],
      "maxAgeSeconds": 3600
    }
  ]
}