{
  "name": "afenda-crud",
  "version": "0.1.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "default": "./src/index.ts"
    },
    "./internal": {
      "types": "./dist/internal.d.ts",
      "import": "./dist/internal.mjs",
      "require": "./dist/internal.js",
      "default": "./src/internal.ts"
    }
  },
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --cache --concurrency=auto",
    "lint:ci": "eslint . --ext .js,.jsx,.ts,.tsx --rule 'import/no-cycle: error'",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --cache --fix",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "afenda-canon": "workspace:*",
    "afenda-database": "workspace:*",
    "afenda-logger": "workspace:*",
    "afenda-workflow": "workspace:*",
    "drizzle-orm": "catalog:",
    "fast-json-patch": "catalog:",
    "ioredis": "catalog:"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "@types/pg": "catalog:",
    "afenda-eslint-config": "workspace:^",
    "afenda-typescript-config": "workspace:*",
    "pg": "catalog:",
    "tslib": "catalog:",
    "tsup": "catalog:",
    "typescript": "catalog:"
  }
}